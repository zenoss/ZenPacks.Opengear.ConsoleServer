<object id='/zport/dmd/Events/App/Conn/instances/ogconnEventOccurred' module='Products.ZenEvents.EventClassInst' class='EventClassInst'>
<property type="text" id="transform" mode="w" >
evt.summary = "User '%s' %s port #%d (%s)" % (evt.ogconnEventUsername, ("disconnected from", "connected to")[evt.ogconnEventType == "login"], evt.ogconnEventPortNumber, evt.ogconnEventPortLabel,)
if evt.ogconnEventType == 'logout':
   evt.severity = 0
else:
   evt.severity = 4

evt.eventKey = "%s_port_%d_user_%s" % (evt.device, evt.ogconnEventPortNumber, evt.ogconnEventUsername,)
</property>
<property type="string" id="eventClassKey" mode="w" >
ogconnEventOccurred
</property>
<property type="int" id="sequence" mode="w" >
7
</property>
<property type="string" id="example" mode="w" >
snmp trap ogconnEventOccurred
</property>
</object>
<!-- ('', 'zport', 'dmd', 'Events', 'Environ', 'instances', 'ogsensEventOccurred') -->
<object id='/zport/dmd/Events/Environ/instances/ogsensEventOccurred' module='Products.ZenEvents.EventClassInst' class='EventClassInst'>
<property type="text" id="transform" mode="w" >
type = "Unknown"
device = "Unknown"
value = "Unknown"

if evt.ogsensStatusType == "temp":
   type = "Temperature"
   value = "%s degrees" % (evt.ogsensStatusValue,)
else:
   value = ogsensStatusValue
 
if evt.ogsensStatusDevType == "emd":
   device = "EMD"
elif evt.ogsensStatusDevType == "rpc":
   device = "RPC"
elif evt.ogsensStatusDevType == "ups":
   device = "UPS"

evt.summary = "%s is %s for %s device '%s'" % (type, value, device, evt.ogsensStatusName,)
evt.eventKey = "%s_sensor_%s_%s_%s_%s" % (evt.device, evt.ogsensStatusName, evt.ogsensStatusDevType, evt.ogsensStatusType, evt.ogsensStatusValue,)
</property>
<property type="string" id="eventClassKey" mode="w" >
ogsensEventOccurred
</property>
<property type="int" id="sequence" mode="w" >
7
</property>
<property type="string" id="example" mode="w" >
snmp trap ogsensEventOccurred
</property>
</object>
