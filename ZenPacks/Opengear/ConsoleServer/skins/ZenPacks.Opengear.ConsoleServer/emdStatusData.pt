<tal:block metal:use-macro="here/templates/macros/page2">
<tal:block metal:fill-slot="contentPane">

<tal:block 
    tal:define="tableName string:emdStatusList;
    objects here/EmdStat/objectValuesAll;
    tabletitle string:Opengear EMD Status;
    batch python:here.ZenTableManager.getBatch(tableName,objects)">
<tal:block metal:use-macro="here/zenuimacros/macros/zentable">
<tal:block metal:fill-slot="zentablecontents">

<!-- BEGIN TABLE CONTENTS -->
    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'ogEmdStatusName','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'ogEmdStatusTemp','Temperature')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'ogEmdStatusHumidity','Humidity')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'ogEmdStatusAlertCount','Alert Count')"/>
    </tr>
    <tal:block tal:repeat="gd batch">
    <tr tal:define="odd repeat/gd/odd; link gd/managedDeviceLink"
        tal:attributes="class python:test(odd, 'odd', 'even')">
	<!--
        <td class="tablevalues">
	    <span tal:content="structure gd/urlLink"/>
	    <span tal:condition="link" tal:omit-tag>
                 (<span tal:replace="structure gd/managedDeviceLink"/>)
            </span>
        </td>
	-->
        <td class="tablevalues" tal:content="gd/ogEmdStatusName"/>
        <td class="tablevalues" tal:content="gd/ogEmdStatusTemp"/>
        <td class="tablevalues" tal:content="gd/ogEmdStatusHumidity"/>
        <td class="tablevalues" tal:content="gd/ogEmdStatusAlertCount"/>
    </tr>
    </tal:block>
    <tr>
        <td colspan="8" class="tableheader" align='center'>
        <form metal:use-macro="here/zenTableNavigation/macros/navtool"></form>
        </td>
    </tr>
<!-- END TABLE CONTENTS -->

</tal:block>
</tal:block>
</tal:block>

</tal:block>
</tal:block>

